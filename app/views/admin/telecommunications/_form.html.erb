<%= simple_form_for @telecommunication, url: url do |f| %>
  <%= f.input :name %>
  <%= image_tag @telecommunication.logo.url(:medium) if @telecommunication.logo.present? %>
  <%= f.input :logo, hint: "會自動裁減為140x40(px)", as: :file %>
  <%= f.button :submit, "提交", class: "btn btn-primary" %>
<% end %>
