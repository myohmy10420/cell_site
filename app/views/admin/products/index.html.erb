<div class="product">
  <%= link_to new_admin_product_path do %>
    <button type="button" class="btn btn-primary btn-add-product">新增商品</button>
  <% end %>
  <div class="product_list">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">商品名稱</th>
          <th scope="col">標題語</th>
          <th scope="col">市價</th>
          <th scope="col">折扣價</th>
          <th scope="col">狀態</th>
          <th scope="col"></th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>iphone</td>
          <td>超便宜</td>
          <td>10000</td>
          <td>8000</td>
          <td>上架中</td>
          <td>
            <%= image_tag "icons/pencil" %>
            <%= image_tag "icons/x" %>
          </td>
        </tr>
      </tbody>
    </table>
  </div>
</div>
